<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.encrm.entity">
	
	<!-- 查询所有课程信息 -->
	<select id="queryAllSystemMessage" parameterType="java.util.Map" resultType="com.encrm.system.entity.Systemmessage">
		select
			s.systemmessageId,s.messagetitle,s.messageContent,s.issend,s.send_time
		from en_systemmessage s
	</select>
	
</mapper> 
